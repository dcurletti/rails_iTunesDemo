<h1>Users</h1>
<div class="panel panel-default" id="user_table">
  <table class="table">
    <tr>
      <th>Name<th>
      <th>Password Digest<th>
      <th>Session Token<th>
    </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></a><td>
        <td><%= user.password_digest %><td>
        <td><%= user.session_token %><td>
      </tr>
    <% end %>
  </table class="table">
</div>
