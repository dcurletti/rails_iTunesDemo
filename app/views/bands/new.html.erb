
<div id="name" class="col-md-2 col-md-offset-5">
  <h1>Band Creation</h1>
  <form id="form_id" action= "<%= bands_url %>" method= "POST" class="navbar-form">
    <div id="test" class="form-group">

      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
      
      <label for="band_name">Band name:</label>
      <input class="form-control" type="text" name="band[name]" id="band_name" value="<%= @band.name %>">
      <br>

      <input class="btn btn-primary btn-sm btn-block" type="submit" value="Create Band">
    </div>
  </form>

</div>
